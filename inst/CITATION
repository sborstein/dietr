citHeader("To cite dietr in publications please use:")


citEntry(entry = "article",
         title   = "dietr: an R package for calculating fractional trophic levels from quantitative and qualitative diet data",
         author  = "Samuel R. Borstein",
		 month = 'oct',
         year    = "2020",
		 publisher = 'Springer',
		 journal = 'Hydrobiologia',
         url     = "https://github.com/sborstein/dietr",
         
         textVersion =
           paste("Borstein SR. 2020. dietr: an R package for calculating fractional trophic levels from quantitative and qualitative diet data. Hydrobiologia 847:4285-4294. 10.1007/s10750-020-04417-5")
)
